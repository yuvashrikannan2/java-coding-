class DataProcessor implements Runnable {
    private String processorName;
    
    public DataProcessor(String processorName) {
        this.processorName = processorName;
    }
    
    @Override
    public void run() {
        System.out.println(processorName + " started");
        for (int i = 1; i <= 3; i++) {
            System.out.println(processorName + " processing batch " + i);
            try {
                Thread.sleep(800);
            } catch (InterruptedException e) {
                Thread.currentThread().interrupt();
            }
        }
        System.out.println(processorName + " completed");
    }
}

public class ConcurrentSystem {
    public static void main(String[] args) {
        Thread processor1 = new Thread(new DataProcessor("CPU-Core1"));
        Thread processor2 = new Thread(new DataProcessor("CPU-Core2"));
        Thread processor3 = new Thread(new DataProcessor("CPU-Core3"));
        
        processor1.start();
        processor2.start();
        processor3.start();
    }
}
